<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="545.0" prefWidth="954.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LogEditor">
   <children>
      <VBox layoutY="29.0"/>
      <VBox prefHeight="36.0" prefWidth="954.0">
         <children>
            <MenuBar>
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem fx:id="closeApp" mnemonicParsing="false" onAction="#closeProg" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                        <MenuItem fx:id="defMenu" mnemonicParsing="false" onAction="#openDef" text="Definitions" />
                        <MenuItem mnemonicParsing="false" text="Add Project" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help" />
              </menus>
            </MenuBar>
         </children>
      </VBox>
  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
         <children>
            <ChoiceBox fx:id="projSel" layoutX="38.0" layoutY="56.0" prefHeight="29.0" prefWidth="266.0" />
            <ChoiceBox fx:id="logSel" layoutX="38.0" layoutY="125.0" prefHeight="25.0" prefWidth="472.0" />
            <ChoiceBox fx:id="lifeCylcleStep" layoutX="38.0" layoutY="239.0" prefWidth="150.0" />
            <ChoiceBox fx:id="plan" layoutX="268.0" layoutY="290.0" prefHeight="26.0" prefWidth="265.0" />
            <ChoiceBox fx:id="effortCat" layoutX="38.0" layoutY="290.0" prefWidth="150.0" />
            <Button fx:id="updateEntry" layoutX="606.0" layoutY="290.0" mnemonicParsing="false" onMouseClicked="#saveUpdate" text="Update Entry" />
            <Button fx:id="deleteEntry" layoutX="38.0" layoutY="348.0" mnemonicParsing="false" onMouseClicked="#deleteEntry" text="Delete Entry" />
            <Button fx:id="splitEntry" layoutX="38.0" layoutY="399.0" mnemonicParsing="false" text="Split into Two Entries" />
            <Button fx:id="consoleReturn" layoutX="508.0" layoutY="399.0" mnemonicParsing="false" onMouseClicked="#returnConsole" text="Proceed to Effort Log Console" visible="false" />
            <Button fx:id="clearLog" layoutX="522.0" layoutY="58.0" mnemonicParsing="false" text="Clear This Effort Log" />
            <DatePicker fx:id="date" layoutX="38.0" layoutY="180.0" onAction="#selectDate" />
            <TextField fx:id="startTime" layoutX="292.0" layoutY="180.0" onAction="#startTime" />
            <TextField fx:id="endTime" layoutX="559.0" layoutY="180.0" onAction="#endTime" />
            <Label fx:id="invalidDate" layoutX="39.0" layoutY="206.0" prefHeight="17.0" prefWidth="108.0" text="Invalid Date" textFill="RED" visible="false" />
            <Label fx:id="endtimeError" layoutX="414.0" layoutY="230.0" text="End time must occur after start time" textFill="RED" visible="false" />
            <Label fx:id="startErrorLabel" layoutX="292.0" layoutY="215.0" text="Invalid Start Time Entered" textFill="RED" visible="false" />
            <Label fx:id="endErrorLabel" layoutX="559.0" layoutY="215.0" text="Invalid End Time Entered" textFill="RED" visible="false" />
            <Label layoutX="38.0" layoutY="39.0" prefHeight="17.0" prefWidth="101.0" text="Select a Project:" />
            <Label layoutX="38.0" layoutY="108.0" text="Select an entry to modify:" />
            <Label layoutX="39.0" layoutY="163.0" prefHeight="17.0" prefWidth="37.0" text="Date:" />
            <Label layoutX="292.0" layoutY="163.0" text="Start Time:" />
            <Label layoutX="559.0" layoutY="163.0" text="Stop Time:" />
            <Label layoutX="38.0" layoutY="224.0" text="Life Cycle Step:" />
            <Label layoutX="38.0" layoutY="273.0" text="Effort Category:" />
            <Label layoutX="268.0" layoutY="273.0" text="Plan:" />
            <Label fx:id="savedUpdateLbl" layoutX="606.0" layoutY="316.0" text="Updates Saved!" visible="false" />
            <Label fx:id="invalidInputLbl" layoutX="601.0" layoutY="316.0" text="Please correct errors before saving!" textFill="RED" visible="false" />
            <DialogPane fx:id="deletePane" layoutX="221.0" layoutY="108.0" prefHeight="215.0" prefWidth="360.0" visible="false" />
            <Label fx:id="deleteAskLabel" layoutX="293.0" layoutY="155.0" text="Are you sure you want to delete the log?" visible="false" />
            <Button fx:id="yesBtn" layoutX="292.0" layoutY="240.0" mnemonicParsing="false" onMouseClicked="#confirmDelete" text="Yes" visible="false" />
            <Button fx:id="cancelBtn" layoutX="404.0" layoutY="241.0" mnemonicParsing="false" onMouseClicked="#cancelDelete" text="Cancel" visible="false" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
